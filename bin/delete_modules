#!/usr/bin/env node

const CanvasApi  = require('./cass/canvas-api')

async function main() {
  const canvas   = new CanvasApi(undefined, {verbose: true})
  const modules  = await canvas.getModules()
  const promises = modules.map(each => canvas.deleteModule(each.id))
  await Promise.all(promises)
}

main().then(_ => 0, console.log)
